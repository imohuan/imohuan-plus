  },
  "dependencies": {
    "npminstall": "^7.4.0"
  },
  "devDependencies": {
    "fs-extra": "^11.1.0",
    "@types/fs-extra": "^11.0.1",
    "@imohuan-plus/info": "workspace:*",
    "@imohuan-plus/utils-common": "workspace:*"
  }
}
